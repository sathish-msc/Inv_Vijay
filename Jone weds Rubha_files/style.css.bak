/*** count down****/
.countblink{
  width:90px;
  height:90px;
  animation-name: example;
  animation-duration: 1s;
  animation-iteration-count: infinite;
  border-radius:50%;
  float:left;
  margin:20px; 

}
@keyframes example {
  from {background-color: #fa86c4;box-shadow: 2px 2px 7px #fa86c4 ,0px 0px 14px #fa86c4, 0px 0px 21px #fa86c4,0px 0px 28px #fa86c4,0px 0px 35px #fa86c4;}
  to {background-color: #fc46aa;box-shadow: 2px 2px 7px #fc46aa ,0px 0px 14px #fc46aa, 0px 0px 21px #fc46aa,0px 0px 28px #fc46aa,0px 0px 35px #fc46aa;}
}
.countpara{
    font-size:18px;
    padding-top: 15px;
    color: white;
    font-weight: 800;
  }
@media(min-width: 1000px){
	.countblink{
		width:180px;
  		height:180px;

	}
	.countpara{
		font-size:30px;
		margin-top: 25px;
	}
	button.date{
		width:50%px;height: 40px;
		padding: 5px;
		font-size: 20px;
	}
}

