/***couple****/
.couple-wrap {
  width: 90%;
  margin: 0 auto;
  position: relative;
}
.couplefont{
	font-family: Brush Script MT;
}
@media screen and (max-width: 768px) {
  .couple-wrap {
    width: 100%;
  }
}

.js .animate-box {
  opacity: 0;
}

.couple-half {
  width: 50%;
  float: left;
}
@media screen and (max-width: 768px) {
  .couple-half {
    width: 100%;
  }
   .heart{
  	display: none;
  }
}
.couple-half h3 {
  font-family: "Sacramento", Arial, serif;
  color: #ff1694;
  font-weight: bold;
  font-size: 30px;
}
.couple-half .groom,
.couple-half .bride {
  float: left;
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  -ms-border-radius: 50%;
  border-radius: 50%;
  width: 150px;
  height: 150px;
 
}
.couple-half .groom img,
.couple-half .bride img {
  width: 150px;
  height: 150px;
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  -ms-border-radius: 50%;
  border-radius: 50%;
  box-shadow: 2px 2px 7px pink ,0px 0px 14px #fe7f9c;
}
@media screen and (max-width: 480px) {
  .couple-half .groom,
  .couple-half .bride {
    width: 100%;
    height: 140px;
  }
  .couple-half .groom img,
  .couple-half .bride img {
    width: 120px;
    height: 120px;
    margin: 0 auto;
  }
}
.couple-half .groom {
  float: right;
  margin-right: 5px;
}
.couple-half .bride {
  float: left;
  margin-left: 5px;
}
.couple-half .desc-groom {
  padding-right: 180px;
  text-align: right;
}
.couple-half .desc-bride {
  padding-left: 180px;
  text-align: left;
}
@media screen and (max-width: 480px) {
  .couple-half .groom,
  .couple-half .bride {
    margin-left: 0;
    margin-right: 0;
  }
  .couple-half .desc-groom {
    padding-right: 0;
    text-align: center;
  }
  .couple-half .desc-bride {
    padding-left: 0;
    text-align: center;
  }
  .heart{
  	display: none;
  }
  .bride img{
  	display: flex;
  	justify-content: center;
  }
  .groom img{
  	display: flex;
  	justify-content: center;
  }
}
.heart {
  position: absolute;
  top: 4em;
  left: 0;
  right: 0;
  z-index: 99;
  animation: pulse 1s ease infinite;
}
.heart i {
  font-size: 20px;
  background: #fff;
  padding: 20px;
  color: #fc46aa;
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  -ms-border-radius: 50%;
  border-radius: 50%;
}
